local Rayfield = loadstring(game:HttpGet("https://sirius.menu/rayfield"))()

local Window = Rayfield:CreateWindow({
    Name            = "Lukman",
    LoadingTitle    = "Rayfield Terbaik",
    LoadingSubtitle = "by Lukman",
    ToggleUIKeybind = Enum.KeyCode.K,
    ConfigurationSaving = {
        Enabled   = true,
        FolderName= krnl,
        FileName  = "Lukmangans"
    }
})

local MainTab     = Window:CreateTab("Main", nil)
local MainSection = MainTab:CreateSection("🎣")

local ReplicatedStorage = game:GetService("ReplicatedStorage")
local Players           = game:GetService("Players")

local net = ReplicatedStorage:WaitForChild("Packages")
    :WaitForChild("_Index")
    :WaitForChild("sleitnick_net@0.2.0")
    :WaitForChild("net")

local player     = Players.LocalPlayer
local CHARGE_AMT = -999999999999.99999999999

MainTab:CreateToggle({
    Name          = "Mancing Mang",
    CurrentValue  = false,
    Flag          = "AutoFishToggle",
    Callback      = function(on)
        _G.FISH = on

        while _G.FISH do
            net:WaitForChild("RF/ChargeFishingRod"):InvokeServer(CHARGE_AMT)

              net:WaitForChild("RF/RequestFishingMinigameStarted"):InvokeServer(0, -1, -9999999.99999)

              task.wait(_G.MancingDelay)
        end
    end
})

local Input = MainTab:CreateInput({
   Name = "Delay nya mang",
   CurrentValue = "",
   PlaceholderText = "Masukin Mang",
   RemoveTextAfterFocusLost = false,
   Flag = "Setting Delay Mancing",
   Callback = function(Text)
        local num = tonumber(Text)
        if num and num >= -1 then
        _G.MancingDelay = num
     end
   end
})


local Button = MainTab:CreateButton({
   Name = "Debug pancing",
   Callback = function()
   net:WaitForChild("RF/CancelFishingInputs"):InvokeServer()
   end,
})

MainTab:CreateToggle({
    Name          = "Tarik Mang",
    CurrentValue  = false,
    Flag          = "AutoReelToggle",
    Callback      = function(on)
          _G.Reel = on

        while _G.Reel do

                net:WaitForChild("RE/FishingCompleted"):FireServer()

              task.wait(_G.ReelDelay)
        end
    end
})

local Input = MainTab:CreateInput({
   Name = "Delay nya mang",
   CurrentValue = "",
   PlaceholderText = "Masukin Mang",
   RemoveTextAfterFocusLost = false,
   Flag = "Setting Delay Reel",
   Callback = function(Text)
        local num = tonumber(Text)
        if num and num >= -1 then
        _G.ReelDelay = num
     end
   end
})


local AutoTab     = Window:CreateTab("Auto", nil)
local AutoSection = AutoTab:CreateSection("🔁")

AutoTab:CreateToggle({
    Name          = "Auto Sell Mang",
    CurrentValue  = false,
    Flag          = "AutoSellToggle",
    Callback      = function(on)
        _G.SELL = on

        while _G.SELL do
            net:WaitForChild("RF/SellAllItems"):InvokeServer()
            task.wait(_G.SellDelay)
        end
    end
})

local Input = AutoTab:CreateInput({
   Name = "Delay nya mang",
   CurrentValue = "",
   PlaceholderText = "Masukin Mang",
   RemoveTextAfterFocusLost = false,
   Flag = "Setting Delay Sell",
   Callback = function(Text)
        local num = tonumber(Text)
        if num and num >= 0.01 then
        _G.SellDelay = num
     end
   end
})

AutoTab:CreateToggle({
    Name          = "Auto Weather☁️+✨ Mang",
    CurrentValue  = false,
    Flag          = "AutoWeatherToggle",
    Callback      = function(on)
        _G.WEATHER = on

        while _G.WEATHER do
            net:WaitForChild("RF/PurchaseWeatherEvent"):InvokeServer("Cloudy")
            net:WaitForChild("RF/PurchaseWeatherEvent"):InvokeServer("Radiant")
            task.wait(_G.WeatherDelay)
        end
    end
})

local Input = AutoTab:CreateInput({
   Name = "Delay nya mang",
   CurrentValue = "",
   PlaceholderText = "Masukin Mang",
   RemoveTextAfterFocusLost = false,
   Flag = "Setting Delay Weather",
   Callback = function(Text)
        local num = tonumber(Text)
        if num and num >= 0.01 then
        _G.WeatherDelay = num
     end
   end
})
