local Rayfield = loadstring(game:HttpGet("https://sirius.menu/rayfield"))()

local Window = Rayfield:CreateWindow({
    Name            = "Lukman",
    LoadingTitle    = "Rayfield Terbaik",
    LoadingSubtitle = "by Lukman",
    ToggleUIKeybind = Enum.KeyCode.K,
    ConfigurationSaving = {
        Enabled   = true,
        FolderName= nil,
        FileName  = "Lukmangans"
    }
})

local MainTab     = Window:CreateTab("Main", nil)
local MainSection = MainTab:CreateSection("üé£")

local ReplicatedStorage = game:GetService("ReplicatedStorage")
local Players           = game:GetService("Players")

local net = ReplicatedStorage:WaitForChild("Packages")
    :WaitForChild("_Index")
    :WaitForChild("sleitnick_net@0.2.0")
    :WaitForChild("net")

local player     = Players.LocalPlayer
local CHARGE_AMT = 1761133690.488849

MainTab:CreateToggle({
    Name          = "Mancing Mang",
    CurrentValue  = false,
    Flag          = "AutoFishToggle",
    Callback      = function(on)
        _G.FISH = on

        while _G.FISH do
            net:WaitForChild("RF/ChargeFishingRod"):InvokeServer(CHARGE_AMT)

            local char = player.Character or player.CharacterAdded:Wait()
            local root = char:WaitForChild("HumanoidRootPart")
            local pos  = root.Position
            net:WaitForChild("RF/RequestFishingMinigameStarted"):InvokeServer(pos.X, pos.Z)

            local completed = net:WaitForChild("RE/FishingCompleted")
            completed:FireServer()

            task.wait(0.3)
        end
    end
})

local AutoTab     = Window:CreateTab("Auto", nil)
local AutoSection = AutoTab:CreateSection("üîÅ")

AutoTab:CreateToggle({
    Name          = "Auto Sell Mang",
    CurrentValue  = false,
    Flag          = "AutoSellToggle",
    Callback      = function(on)
        _G.SELL = on

        while _G.SELL do
            net:WaitForChild("RF/SellAllItems"):InvokeServer()
            task.wait(600)
        end
    end
})

AutoTab:CreateToggle({
    Name          = "Auto Weather‚òÅÔ∏è+‚ú® Mang",
    CurrentValue  = false,
    Flag          = "AutoWeatherToggle",
    Callback      = function(on)
        _G.WEATHER = on

        while _G.WEATHER do
     
local args = {
	"Cloudy"
}
net:WaitForChild("RF/PurchaseWeatherEvent"):InvokeServer(unpack(args))
local args = {
	"Radiant"
}
net:WaitForChild("RF/PurchaseWeatherEvent"):InvokeServer(unpack(args))
 task.wait(10)
        end
    end
})
